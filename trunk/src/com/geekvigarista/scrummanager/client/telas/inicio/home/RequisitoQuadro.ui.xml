<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with type="com.geekvigarista.scrummanager.client.i18n.Mensagem"
		field="msg"></ui:with>
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel width="120px">
			<g:Cell horizontalAlignment="ALIGN_CENTER">
				<g:Label text="titulo" ui:field="titulo" styleName="titulo3"/>
			</g:Cell>
			<g:Cell horizontalAlignment="ALIGN_RIGHT">
				<g:Hyperlink text="{msg.detalhes}" ui:field="detalhes"></g:Hyperlink>
			</g:Cell>
			<g:Cell horizontalAlignment="ALIGN_CENTER">
				<g:HorizontalPanel>
				<g:Button text="&lt;-" ui:field="previous"/>
				<g:Button text="-&gt;" ui:field="next"/>
			</g:HorizontalPanel>
			</g:Cell>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 