<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel>
		<g:Grid>
			<g:row>
				<g:customCell>
					<g:Label text="Descricao" />
				</g:customCell>
				<g:customCell>
					<g:RichTextArea ui:field="descricao" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Encaminhar para" />
				</g:customCell>
				<g:customCell>
					<g:ListBox ui:field="stakeholder" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="TempoGasto" />
				</g:customCell>
				<g:customCell>
					<g:IntegerBox ui:field="tempoGasto" />
				</g:customCell>
			</g:row>
		</g:Grid>
		<g:HorizontalPanel horizontalAlignment="ALIGN_RIGHT"
			width="100%">
			<g:Cell horizontalAlignment="ALIGN_RIGHT">
				<g:Button width="100px" ui:field="salvar">OK</g:Button>
			</g:Cell>
		</g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 